{
  "manifest_version": 2,
  "name": "SAIS Timetable",
  "description": "Adds a timetable of your classes on the shopping cart page.",
  "version": "1.0.1",
  "content_scripts": [
    {
      "matches": ["https://*.up.edu.ph/*/*", "http://*.up.edu.ph/*/*"],
      "js": ["bundle.js"],
      "css": ["style.css"],
      "all_frames": true,
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
