{
    "manifest_version": 2,
    "name": "SAIS Timetable",
    "description": "Adds a timetable of your classes on the shopping cart page.",
    "version": "0.3.1",
    "content_scripts": [{
        "matches": ["https://sais.up.edu.ph/*/*"],
        "css": ["style.css"],
        "js": ["jquery-3.1.0.min.js", "scheda.js", "onDomChange.js", "script.js"],
        "all_frames": true
    }],
    "permissions": [
        "activeTab"
    ]
}
